
public class Main
{
	public static void main(String[] args) {
	    int a[]={0,1,0,1,0,0,1,1};
	    int c=0;
	    for(int k=0;k<a.length;k++){
	        if(a[k]==0){
	            c++;
	        }
	        
	    }
		int r[]=new int[a.length];
		int i=0;
		while(i<c){
		    r[i]=0;
		    i++;
		}
		
		while(c<a.length){
		    r[c]=1;
		    c++;
		}
		for(int k=0;k<r.length;k++){
	        System.out.println(r[k]);
	    }
	}
}
